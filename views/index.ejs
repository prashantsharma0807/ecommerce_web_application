<section id="mid">
  <div class="container">
      <div class="row">
          <%recordToy.forEach((result)=>{%>

              <div class="col-md-3">
                  <div class="card mx-auto" style="width: 18rem;">
                      
                      <img src="/upload/<%=result.img%>" class="card-img-top d-flex mx-auto img-fluid" alt="...">
                      <div class="card-body">
                        <p class="card-text fw-bold"><%=result.item%></p>
                        <p class="card-text"><%=result.desc%></p>
                        <p class="card-text fw-bold">₹<%=result.price%></p>
                        <button id="b1">Add to Cart<i class="bi bi-cart3"></i></button>
                        <button id="b2">Buy now</button>
                      </div>
                  
                    </div>
              </div>

          <%})%>
          
      </div>
  </div>
</section>

